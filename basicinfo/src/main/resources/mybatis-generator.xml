<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>


    <context id="baseset" targetRuntime="MyBatis3Simple" defaultModelType="flat">

        <!-- 根据Mapper生成实体类,xml文件 -->
        <!--<plugin type="tk.mybatis.mapper.generator.MapperPlugin">-->
        <!--<property name="mappers" value="com.ailianshuo.project.utility.MyMapper"/>-->
        <!--</plugin>-->

        <!-- 为了防止生成的代码中有很多注释，比较难看，加入下面的配置控制  -->
        <!-- &lt;!&ndash; 是否去除自动生成的注释 true：是 ： false:否 &ndash;&gt; -->
        <commentGenerator>
            <property name="suppressDate" value="false"/>
            <property name="suppressAllComments" value="false"/>
        </commentGenerator>

        <!-- 连接字符串 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/gpss?characterEncoding=UTF-8"
                        userId="root"
                        password="root">
        </jdbcConnection>

        <!-- 指定生成“entity实体类、mybatis映射xml文件、mapper接口”的具体位置 -->
        <javaModelGenerator targetPackage="com.sinfeeloo.inventory.entity" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="com.sinfeeloo.inventory.mapper" targetProject="src/main/java"
                             type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>


        <!-- 具体要生成的表，如果有多个表，复制这一段，改下表名即可-->
        <!--&lt;!&ndash;商品&ndash;&gt;-->
        <!--<table tableName="lz_base_goods" domainObjectName="Goods" enableCountByExample="false"-->
               <!--enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"-->
               <!--selectByExampleQueryId="false"/>-->
        <!--&lt;!&ndash;仓库&ndash;&gt;-->
        <!--<table tableName="lz_base_repo" domainObjectName="Storage" enableCountByExample="false"-->
               <!--enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"-->
               <!--selectByExampleQueryId="false"/>-->
        <!--&lt;!&ndash;员工&ndash;&gt;-->
        <!--<table tableName="lz_base_employee" domainObjectName="Employee" enableCountByExample="false"-->
               <!--enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"-->
               <!--selectByExampleQueryId="false"/>-->
        <!--&lt;!&ndash;参数&ndash;&gt;-->
        <table tableName="lz_base_param" domainObjectName="Param" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"/>

        <!--图片-->
        <!--<table tableName="lz_base_image" domainObjectName="Image" enableCountByExample="false"-->
               <!--enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"-->
               <!--selectByExampleQueryId="false"/>-->

    </context>
</generatorConfiguration>